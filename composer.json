{
  "name": "uuf6429/dfsc",
  "description": "Docker \"Full State\" Composer",
  "minimum-stability": "stable",
  "license": "MIT",
  "require": {
    "php": ">=5.6",
    "roave/security-advisories": "dev-master",
    "symfony/console": "^3.3",
    "league/json-guard": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7",
    "adlawson/vfs": "^0.12.1"
  },
  "autoload": {
    "psr-4": {
      "uuf6429\\DFSC\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "uuf6429\\DFSC\\": "tests"
    }
  },
  "bin": ["dfsc"],
  "suggest": {
    "uuf6429/wait2services": "Tool for waiting until all required services are online."
  }
}
